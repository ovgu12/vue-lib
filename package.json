{
  "name": "vue-lib-sfc-example",
  "version": "2.0.0",
  "description": "Example of building Vue SFC with Typescript",
  "scripts": {
    "start": "yarn serve",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "build:lib": "vue-cli-service build --target lib ./src/index.ts"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.1.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/vue": "^2.0.0",
    "@vue/cli": "^3.8.2",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.1",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "ISC",
  "lint-staged": {
    "*.{vue,js,md,ts,css,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "./dist/vue-lib-sfc-example.umd.js"
}